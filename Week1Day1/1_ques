import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a, b;
        a = scan.nextInt();
        b = scan.nextInt();
        if (a<0 || b<0){
            System.out.print("Invalid Input");
        }
        else{
            System.out.print(a + "-" + b);
        }
    }
}
